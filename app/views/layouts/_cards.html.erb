<div class="service-container">

   <% @user_services.each do |s| %>

    <div class="card-service">

    <div class="service">
      <h2><%= s.title %><h2>
    </div>

    <div class="service">
      <%= s.price %>
    </div>

    <div class="service">
      <%= s.description%>
    </div>


     <div class="service">
      <%= s.location %>
    </div>

  </div>
  <% end %>

</div>
