
<div class="container">
  <div class="row">
    <% @services.each do |s| %>

    <div class="col col-xs-12 col-sm-4">

    <div class="card">
      <div class="card-category"><%= s.location %></div>
      <div class="card-description">
        <h2><%= s.title %></h2>
        <p><%= s.category %></p>
      </div>
      <a class="card-link" href="#" ></a>

    </div>


    </div>
    <% end %>

  </div>
</div>



